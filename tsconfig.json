{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "exactOptionalPropertyTypes": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "noUncheckedSideEffectImports": true,
    "verbatimModuleSyntax": true
  },
  "include": [
    "**/*",
    ".nuxt/types/**/*"
  ],
  "exclude": [
    "node_modules",
    "dist",
    ".output"
  ]
}