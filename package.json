{
  "name": "marden-seo-audit",
  "version": "2.0.1",
  "description": "Marden SEO Audit - Professional SEO Analysis Platform built with Nuxt 3",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "start": "node .output/server/index.mjs",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "nuxt typecheck",
    "test": "vitest",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:coverage:final": "vitest run --coverage --reporter=verbose",
    "test:integration": "vitest run tests/integration",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:visual": "playwright test tests/e2e/visual",
    "test:e2e:cross-browser": "playwright test tests/e2e/cross-browser",
    "test:e2e:user-flows": "playwright test tests/e2e/user-flows",
    "test:integration:live": "vitest run tests/integration/live",
    "test:performance": "npx lhci autorun",
    "test:performance:load": "playwright test tests/performance",
    "test:accessibility": "playwright test tests/accessibility",
    "test:visual:update": "playwright test tests/e2e/visual --update-snapshots",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:all:phase3": "npm run test:unit && npm run test:integration && npm run test:integration:live && npm run test:e2e && npm run test:accessibility && npm run test:performance:load",
    "test:ci": "npm run lint && npm run type-check && npm run test:all",
    "test:ci:phase3": "npm run lint && npm run type-check && npm run test:all:phase3",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write .",
    "security:audit": "node scripts/security-audit.js",
    "security:deps": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix",
    "security:check": "npm run security:audit && npm run security:deps",
    "production:check": "NODE_ENV=production npm run security:check && npm run type-check",
    "clean": "rm -rf .nuxt .output dist node_modules/.cache"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@eslint/js": "^9.30.0",
    "@lhci/cli": "^0.15.1",
    "@nuxt/devtools": "latest",
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxt/test-utils": "^3.9.0",
    "@playwright/test": "^1.53.1",
    "@tailwindcss/postcss": "^4.1.11",
    "@testing-library/vue": "^8.1.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.10.6",
    "@types/pixelmatch": "^5.2.6",
    "@types/pngjs": "^6.0.5",
    "@types/supertest": "^6.0.3",
    "@types/ua-parser-js": "^0.7.39",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/coverage-v8": "^1.6.1",
    "@vue/test-utils": "^2.4.3",
    "axe-core": "^4.10.3",
    "eslint": "^8.57.1",
    "eslint-plugin-vue": "^10.2.0",
    "happy-dom": "^13.3.1",
    "lighthouse": "^12.7.0",
    "nuxt": "^3.9.3",
    "pixelmatch": "^7.1.0",
    "playwright-core": "^1.56.1",
    "pngjs": "^7.0.0",
    "prettier": "^3.6.2",
    "supertest": "^7.1.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0",
    "vue-eslint-parser": "^10.1.4",
    "vue-tsc": "^2.2.10",
    "wait-on": "^8.0.3"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.20.9",
    "@heroicons/vue": "^2.2.0",
    "@nuxt/content": "^2.9.0",
    "@nuxtjs/supabase": "^1.1.6",
    "@nuxtjs/tailwindcss": "^6.10.1",
    "@pinia/nuxt": "^0.5.1",
    "@supabase/supabase-js": "^2.39.3",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@vueuse/nuxt": "^10.7.1",
    "axios": "^1.6.5",
    "bullmq": "^5.64.1",
    "chart.js": "^4.4.1",
    "cheerio": "^1.0.0-rc.12",
    "gsap": "^3.13.0",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "lottie-web": "^5.13.0",
    "pinia": "^2.1.7",
    "robots-parser": "^3.0.1",
    "ua-parser-js": "^1.0.37",
    "uuid": "^9.0.1",
    "vue-chartjs": "^5.3.2",
    "xml2js": "^0.6.2",
    "zod": "^3.22.4"
  },
  "engines": {
    "node": ">=20.18.1",
    "npm": ">=10.0.0"
  }
}
